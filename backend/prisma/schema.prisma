generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model profiles {
  id           String   @id(map: "users_pkey") @unique @db.Uuid
  name         String?
  bio          String?
  skills       String[]
  email        String   @unique @db.VarChar
  phone_number String?
  username     String
}
